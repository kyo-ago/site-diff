{
    "name": "site-diff",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "/html/browser_action.html"
    },
    "background": {
      "scripts": ["/js/background.js"],
      "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage",
        "<all_urls>"
    ],
    "web_accessible_resources" : [
        "/web_accessible_resources/index.html"
    ]
}
