{
    "name": "site-diff",
    "version": "0.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "/html/browser_action.html"
    },
    "background": {
        "scripts": [
            "/js/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage",
        "<all_urls>"
    ]
}